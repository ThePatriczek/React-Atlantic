{
  "name": "react-atlantic",
  "version": "0.3.3",
  "description": "React Atlantic UI Library",
  "main": "index.js",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "prepublishOnly": "npm run test && tsc -p tsconfig.publish.json && build-storybook -o docs",
    "test": "CI=true node scripts/tests.jsx --env=jsdom",
    "test:watch": "node scripts/tests.jsx --env=jsdom"
  },
  "pre-commit": [
    "test"
  ],
  "babel": {
    "plugins": [
      "@babel/transform-flow-strip-types"
    ],
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "extends": "react-app",
    "globals": {
      "resolveOwn": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "impliedStrict": true,
        "jsx": true
      }
    },
    "rules": {
      "prefer-template": 0,
      "semi": [
        1,
        "never"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThePatriczek/React-Atlantic.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ThePatriczek/React-Atlantic/issues"
  },
  "homepage": "https://github.com/ThePatriczek/React-Atlantic#readme",
  "dependencies": {
    "polished": "^3.4.1",
    "react-select": "^3.0.8",
    "react-tooltip": "^3.11.1",
    "styled-components": "^5.0.0-rc.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@storybook/addon-actions": "5.2.0",
    "@storybook/addon-info": "5.2.0",
    "@storybook/addon-knobs": "5.2.0",
    "@storybook/addon-links": "5.2.0",
    "@storybook/addon-notes": "5.2.0",
    "@storybook/addon-viewport": "5.2.0",
    "@storybook/addons": "5.2.0",
    "@storybook/react": "5.2.0",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "@types/react-color": "^3.0.1",
    "@types/react-dom": "^16.9.0",
    "@types/react-select": "^3.0.5",
    "@types/react-tooltip": "^3.9.3",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "4.1.8",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.1",
    "babel-jest": "17.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "dotenv": "latest",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.6.0",
    "expect": "^24.9.0",
    "jest": "^24.9.0",
    "jest-mock": "^24.9.0",
    "jest-styled-components": "^7.0.0-beta.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-docgen-typescript-loader": "^3.2.1",
    "react-dom": "^16.9.0",
    "react-test-renderer": "latest",
    "storybook-addon-specifications": "^2.1.5",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "browser": false,
    "moduleNameMapper": {
      "^styled-components": "<rootDir>/node_modules/styled-components"
    },
    "setupFiles": [
      "<rootDir>/config/jest/storybook.js",
      "<rootDir>/config/enzymeAdapter.js"
    ],
    "setupFilesAfterEnv": [
      "./setupTest.js"
    ],
    "testEnvironment": "node",
    "cacheDirectory": "test/.cache",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "test/.coverage",
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "globals": {},
    "notify": false,
    "resetMocks": false
  }
}
